{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "../node_modules/jsdoc-export-default-interop/dist/index",
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true
  }
}
